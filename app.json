{
  "expo": {
    "scheme": "allcoaching",
    "name": "allcoaching",
    "slug": "allcoaching",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff00",
      "translucent": true
    },
    "notification": {
      "icon": "./assets/noti.png",
      "color": "#D41422",
      "androidMode": "collapse",
      "androidCollapsedTitle": "collapse"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.xyz.allcoaching",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.allcoaching.in"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "",
    "plugins": [
      "expo-file-system",
      "expo-navigation-bar",
      "expo-image-picker"
    ]
  }
}
